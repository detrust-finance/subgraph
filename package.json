    "dashdash": "^1.14.1",
    "ecc-jsbn": "^0.1.2",
    "extsprintf": "^1.3.0",
    "getpass": "^0.1.7",
    "json-schema": "^0.4.0",
    "jsprim": "^1.4.1",
    "psl": "^1.8.0",
    "rimraf": "^3.0.2",
    "sshpk": "^1.16.1",
    "tmp": "^0.2.1",
    "tweetnacl": "^0.14.5",
    "verror": "^1.10.0"
  },
  "devDependencies": {
    "mustache": "^4.2.0"
  }
